// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Challenger {
  id        Int      @id @default(autoincrement())
  userName  String
  userId    Int      @unique
  joinedAt  DateTime @default(now())
}
